<p id="notice"><%= notice %></p>

<h1>Glibraries</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @glibraries.each do |glibrary| %>
      <tr>
        <td><%= link_to 'Show', glibrary %></td>
        <td><%= link_to 'Edit', edit_glibrary_path(glibrary) %></td>
        <td><%= link_to 'Destroy', glibrary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Glibrary', new_glibrary_path %>
